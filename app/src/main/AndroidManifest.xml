<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.it_android.constraintsetdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".LeftAndRightMove" />
        <activity android:name=".TransitionMove" />
        <activity android:name=".HorizonallyMove" />
        <activity android:name=".CenterMove" />
        <activity android:name=".FullScreenMove" />
        <activity android:name=".ExtendMove" />
        <activity android:name=".ExitActivity" />
        <activity android:name=".All" />


    </application>

</manifest>